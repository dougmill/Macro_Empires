[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/dougmill
[access "refs/for/refs/*"]
	read = group Macro Empires Reviewers
	read = group user/dougmill
	push = group Macro Empires Reviewers
	push = group user/dougmill
	label-Code-Review = -2..+2 group Macro Empires Reviewers
	label-Code-Review = -2..+2 group user/dougmill
	label-Verified = -1..+1 group Macro Empires Reviewers
	label-Verified = -1..+1 group user/dougmill
	rebase = group Macro Empires Reviewers
	rebase = group user/dougmill
	removeReviewer = group Macro Empires Reviewers
	removeReviewer = group user/dougmill
	submit = group Macro Empires Reviewers
	submit = group user/dougmill
[access "refs/heads/*"]
	read = group Macro Empires Reviewers
	read = group user/dougmill
	create = group Macro Empires Reviewers
	create = group user/dougmill
	push = group Macro Empires Reviewers
	push = +force group user/dougmill
	pushMerge = group Macro Empires Reviewers
	pushMerge = group user/dougmill
[access "refs/tags/*"]
	push = group Macro Empires Reviewers
	push = +force group user/dougmill
	pushTag = group Macro Empires Reviewers
	pushTag = group user/dougmill
	pushSignedTag = group Macro Empires Reviewers
	pushSignedTag = group user/dougmill
[receive]
	requireChangeId = true
